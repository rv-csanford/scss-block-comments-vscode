{
    "name": "scss-block-comments",
    "displayName": "scss-block-comments",
    "description": "Adds comments to the end of every scss block",
    "version": "0.0.1",
    "publisher": "Charles Sanford",
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.formatScssBlocks"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.formatScssBlocks",
                "title": "Comment SCSS Blocks"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "eslint": "^4.11.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
